      -
        code : |1-
         
         // Reverses the mute position
         Mute = not Mute
         SaveParameter("Mute", Mute)
         
         // If switching to mute mode
         if Mute then 
         	
         	// Reset volume
         	MultimediaVolume(MultimediaControl, 0)
         else
         	
         	// Restores volume (if set to 0, volume is reset to default value)
         	if Volume = 0 then Volume = VOLUME_DEF
         	SLD_VOLUME.ChangeValue( Volume )
         end
         
         // If mute mode is enabled
         if Mute then
         	
         	// Indicates the "mute" mode is enabled
         	BTN_VOLUME.Caption				= WIN_Main.BTN_MUTE.Caption	
         	WIN_Main.BTN_VOLUME.Plane		= WIN_Main.PLANE_HIDDEN
         	WIN_Main.BTN_MUTE.Plane			= WIN_Main.plane_main
         	
         else
         	
         	// 
         	BTN_VOLUME.Caption				= WIN_Main.BTN_VOLUME.Caption	
         	WIN_Main.BTN_VOLUME.Plane		= WIN_Main.plane_main
         	WIN_Main.BTN_MUTE.Plane			= WIN_Main.PLANE_HIDDEN
         end

        code : |1-
         
         // Stores
         Volume = SLD_VOLUME
         SaveParameter("Volume", Volume)
         
         // If mute mode was enabled
         if Mute then 
         	
         	// Disables mute mode
         	Mute				= False
         	
         	// Restores the text of the Volume button
         	BTN_VOLUME.Caption	= WIN_Main.BTN_VOLUME.Caption
         end
         
         // Changes media volume 
         MultimediaVolume(MultimediaControl, Volume)
         
         if Volume = 0 then
         	
         	// Switch to mute mode
         	Mute				= True
         	BTN_VOLUME.Caption	= WIN_Main.BTN_MUTE.Caption
         end
         
         // Display
         PROGBAR_VOLUME				= Volume
         STC_VOLUME					= Volume
         WIN_Main.BTN_VOLUME.ToolTip	= "Volume: [%Volume%]"
        type : 28

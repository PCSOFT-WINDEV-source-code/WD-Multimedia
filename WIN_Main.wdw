      //
      // Automatic procedure:
      // The procedure is run manually, during a call in the code
      // It will be repeated in loop, with a timeout set to 1 hundredth of a second between each call
      // A new timer is created for each call to the procedure
      // 
      
      procedure PlaybackTrackMediaFilePosition()
      
      // Reads the current playback position
      nPosition			is unsigned int	= MultimediaPosition(MM_PLAYER)
      
      duDurationPassed	is duration
      duDurationPassed.InMilliseconds = nPosition
      
      // Shows elapsed and remaining time
      DisplayDuration( STC_TIME_PASSED, duDurationPassed )
      DisplayDuration( STC_TIME_LEFT, MM_PLAYER.TotalDuration )
      
      // Update position	
      SLD_DURATION		= nPosition
      PROGBAR_DURATION	= nPosition

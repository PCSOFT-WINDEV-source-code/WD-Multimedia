     name : EndPlayback
     procedure_id : 1315892282028570927
     type_code : 14
     code : |1-
      // Summary: Finishes the playback
      // Syntax:
      // EndPlayback ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      procedure EndPlayback()
      
      // End of reading
      MM_PLAYER.ReadingInProgress	= False
      MM_PLAYER.PlaybackEnded		= True
      
      // Hides Play button
      BTN_PLAY.Plane				= plane_main
      BTN_PAUSE.Plane				= PLANE_HIDDEN
      IMG_ANIM_MUSIC.Animation	= False
      
      EndAutomatedProcedure(PlaybackTrackMediaFilePosition)
      SLD_DURATION					= SLD_DURATION.MaxValue
      PROGBAR_DURATION				= PROGBAR_DURATION.MaxValue
      
      // Shows remaining and elapsed time
      DisplayDuration( STC_TIME_PASSED, MM_PLAYER.TotalDuration )
      DisplayDuration( STC_TIME_LEFT, 0 )
     type : 458752
   -
     name : PlaybackInitialize
     procedure_id : 1315892282028636463
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      //PlaybackInitialize (<sFile> is string, <bAudioFormat> is boolean)
      //
      // Parameters:
      //	sFile (ANSI string): <specify the role of sFile>
      // 	bAudioFormat (boolean): <specify the role of bAudioFormat>
      // Return value:
      // 	None
      //
      procedure PlaybackInitialize( sFile is string, bAudioFormat is boolean )
      
      // Gets the total duration of the video
      nDuration is unsigned int = MultimediaDuration(MM_PLAYER)
      
      // Stores the total duration
      MM_PLAYER.TotalDuration.EnMillisecondes		= nDuration
      MM_PLAYER.AudioFormat						= bAudioFormat
      
      // Playback progress bar
      SLD_DURATION.MaxValue						= nDuration
      PROGBAR_DURATION.MaxValue					= nDuration  			
      SLD_DURATION.Value							= 0
      PROGBAR_DURATION.Value						= 0
      
      STC_LOGO.Plane								= PLANE_HIDDEN
      STA_INFO.Plane								= PLANE_HIDDEN
      BTN_PLAY.Plane								= plane_main
      BTN_PAUSE.Plane								= PLANE_HIDDEN
      SLD_DURATION.Plane							= plane_main
      PROGBAR_DURATION.Plane						= plane_main
      MM_PLAYER.Plane								= bAudioFormat ? PLANE_HIDDEN else plane_main
      IMG_ANIM_MUSIC.Plane						= bAudioFormat ? plane_main else PLANE_HIDDEN
      IMG_ANIM_MUSIC.Animation					= False
      
      // Shows remaining and elapsed time
      DisplayDuration( STC_TIME_PASSED, 0 )
      DisplayDuration( STC_TIME_LEFT, MM_PLAYER.TotalDuration )
      
      // Name of the video
      STC_MEDIA_FILE_NAME						= fExtractPath(sFile, fFileName)
      STC_MEDIA_FILE_NAME.Plane				= plane_main
      MyWindow.Title							= "WDMultimedia - " + STC_MEDIA_FILE_NAME
      
      // Align the name
      STC_MEDIA_FILE_NAME.HorizontalAlignment	= bAudioFormat ? haCenter else haLeft
      STC_MEDIA_FILE_NAME.Animation			= bAudioFormat
      
      // Enables playback
      EnableElement( BTN_PLAY )
      BTN_PLAY.ExecuteProcess(trtClick)
     type : 458752
   -
     name : PlaybackStop
     procedure_id : 1315892282028701999
     type_code : 14
     code : |1-
      // Summary: Stops a playback
      // Syntax:
      // PlaybackStop ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      procedure PlaybackStop()
      
      // If playback is in progress
      if MM_PLAYER.ReadingInProgress then
      	
      	// Stops reading
      	MultimediaStop(MM_PLAYER)
      	
      	// Unloads the video
      	MultimediaClose(MM_PLAYER)
      	
      end
      
      // Done
      EndPlayback()
     type : 458752

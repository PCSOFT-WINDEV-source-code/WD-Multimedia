      // Summary: Reorganizes the layout
      // Syntax:
      // ReorganizeLayout ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      procedure ReorganizeLayout()
      
      if FullScreen then
      	
      	// Hides 'Full screen' button
      	BTN_FULL_SCREEN.Plane					= PLANE_HIDDEN
      	
      	// Shows the minimize button again
      	BTN_MINIMIZE.Plane						= plane_main
      	
      	// Hides the background of playback controls
      	IMG_PLAYBACK_BAR_BACKGROUND.Plane		= PLANE_HIDDEN
      	
      	// Starts the mouse activity monitoring timer
      	PlaybackControlSensitivityTimer()
      	
      	// Retarts the timer that hides the tools
      	PlaybackHideControls()
      	
      else
      	
      	// Stops the mouse activity tracking timer
      	EndAutomatedProcedure(PlaybackControlSensitivityTimer)
      	EndAutomatedProcedure(PlaybackHideControls)
      	
      	// Hides the button 
      	BTN_MINIMIZE.Plane						= PLANE_HIDDEN
      	
      	// Shows the 'Full screen' button again
      	BTN_FULL_SCREEN.Plane					= plane_main
      	
      	// Shows the background of playback controls again
      	IMG_PLAYBACK_BAR_BACKGROUND.Plane		= plane_main
      	
      	// Shows playback tools again
      	GR_PLAYBACK.Visible						= True
      	
      end
      
      // If mute mode is enabled
      if Mute then
      	
      	// Indicates the "mute" mode is enabled
      	BTN_VOLUME.Plane	= WIN_Main.PLANE_HIDDEN
      	BTN_MUTE.Plane		= WIN_Main.plane_main
      	
      else
      	
      	// 
      	BTN_VOLUME.Plane	= plane_main
      	BTN_MUTE.Plane		= PLANE_HIDDEN
      end
     type : 458752

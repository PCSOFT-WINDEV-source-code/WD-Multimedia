     name : PlaybackControlSensitivityTimer
     internal_properties : CAAAAAgAAAAyctWUXGtNZq0/Ej/jlutuyNCxYU0G5DzS5FP1x3pvpxSPsBowO1I+UeAHtx3jtqUTHJvuY2ou+Dv3Iu+p52S9zWDjNDIfJEYSrKgiaKD7z26TlUphzBWUPWU6p6LEdmWUoNopjVS/MauNjtU=
     procedure_id : 1315892282028046639
     type_code : 14
     code : |1-
      // Summary: Tracks mouse activity to show the playback controls in full screen mode
      // Syntax:
      //PlaybackControlSensitivityTimer ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Automatic procedure:
      // The procedure is run manually, during a call in the code
      // It will be repeated in loop, with 50 hundredths of a second between each call
      // A new timer is created for each call to the procedure
      // 
      
      procedure PlaybackControlSensitivityTimer()
      
      let CPos 	= CursorPos(cpWindow)
      let Y 		= HiWord(CPos)
      
      // The cursor hovers over the playback controls?
      if Y > IMG_PLAYBACK_BAR_BACKGROUND.Y then
      	
      	// Stops the timer that hides the tools
      	EndAutomatedProcedure(PlaybackHideControls)
      	
      	// If the tools are not shown
      	if GR_PLAYBACK.Visible = False then
      		
      		// Shows the tools
      		GR_PLAYBACK.Visible = True
      		
      		// Retarts the timer that hides the tools
      		PlaybackHideControls()
      	end		
      end
     type : 458752
   -
     name : PlaybackHideControls
     internal_properties : CAAAAAgAAAAADZh/4pXzA9Ipy+3Bb8okDW0AI5z+/M/QWy9HeiKnUS2mNblggFpgqnv43yMT0xf4z+OVBeR/RNELVw2vECNsKmKWCKpnHnE9aPAZEpwXyOsHBQgxcfEA
     procedure_id : 1315892282028112175

     name : LoadFile
     internal_properties : CAAAAAgAAACPRk5rc30yFYTsDWfY2nudwNliJi0iZnM/tcvVE3tB+razWXaJgDeAyoZ2/+2YNSzHiy07eLmmy8gVBSLju5N8B9tb305FA5UTDMTlH6kYX10KvxkLjWJ3KwAOtUrutbMtR1LN/GAJGPiqtZiheqXDXhyyjrjBas94Bc+OfykAAw==
     procedure_id : 1315892282028308783
     type_code : 14
     code : |1-
      // Summary: Loads the specified file
      // Syntax:
      //LoadFile (<sFile> is string)
      //
      // Parameters:
      //	sFile (ANSI string): Full file path
      // Return value:
      // 	None
      //
      procedure LoadFile( sFile is string )
      
      // The file does not exist
      if fFileExist(sFile) = False then
      	ToastDisplay("The file does not exist anymore", toastShort, vaMiddle, haCenter)
      	return
      end
      
      // Loads the 4 first bytes
      bufHeader	is buffer	= fLoadBuffer(sFile, 0, 12)
      bAudio		is boolean
      
      switch True
      
      	// AUDIO
      	case bufHeader[ 1 on 3 ] = [ 0x49, 0x44, 0x33 ], ...//														// MP3 + ID3v2
      		bufHeader[ 1 on 2 ]	= [ 0xff, 0xfb ], ...//																// MP3
      		bufHeader[ 1 on 2 ]	= [ 0xff, 0xf3 ], ...//																// MP3 + ID3v1
      		bufHeader[ 1 on 2 ]	= [ 0xff, 0xf2 ], ...//																// MP3 + ID3v1  			
      		
      		bufHeader[ 1 on 4 ]	= [ 0x52, 0x49, 0x46, 0x46 ] _and_ bufHeader[ 9 on 4 ] = [ 0x57, 0x41, 0x56, 0x45 ], .../// WAV  			
      		bufHeader[ 1 on 4 ]	= [ 0x4f, 0x67, 0x67, 0x53 ], ...//													// OGG, OGA
      		bufHeader[ 1 on 8 ]	= [ 0x30, 0x26, 0xb2, 0x75, 0x8e, 0x66, 0xcf, 0x11 ], ...//							// ASF, WMA
      		bufHeader[ 1 on 8 ]	= [ 0xa6, 0xd9, 0x00, 0xaa, 0x00, 0x62, 0xce, 0x6c ], ...//							// ASF, WMA
      		bufHeader[ 1 on 4 ]	= [ 0x66, 0x4c, 0x61, 0x43 ]																// FLAC
      		
      		bAudio				= True  			  					
      		
      	// Unable to determine the type of file using the header bytes: Use the extension
      	case Lower(fExtractPath( sFile, fExtension )[[ 2 to ]]) in ( "wav", "mp3", "ogg", "oga", "asf", "wma", "flac" )
      		
      		bAudio = True
      		
      	// VIDEO
      	case bufHeader[ 1 on 4 ] = [ 0x52, 0x49, 0x46, 0x46 ] _and_ bufHeader[ 9 on 4 ] = [ 0x41, 0x56, 0x49, 0x20 ], .../// AVI
      		bufHeader[ 1 on 4 ]		= [ 0x1a, 0x45, 0xdf, 0xa3 ], ...//													// MKV, WEBM
      		bufHeader[ 1 ]			= [ 0x47 ] _and_ bufHeader[ 0xbc ] = [ 0x47 ], ...//									// MPG, MPEG, TS
      		bufHeader[ 1 on 4 ]		= [ 0x00, 0x00, 0x01, 0xb3 ], ...//													// MPG, MPEG
      		bufHeader[ 1 on 4 ]		= [ 0x00, 0x00, 0x01, 0xba ], ...//													// MPG, MPEG, VOB, M2P
      		bufHeader[ 1 on 8 ]		= [ 0x66, 0x74, 0x79, 0x70, 0x69, 0x73, 0x6f, 0x6d ], ...//							// MP4
      		bufHeader[ 1 on 3 ]		= [ 0x00, 0x00, 0x00 ] _and_ bufHeader[ 5 on 4 ] = [ 0x66, 0x74, 0x79, 0x70 ], ...//	// MP4
      		bufHeader[ 5 on 4 ]		= [ 0x66, 0x74, 0x79, 0x70 ]	_and_ bufHeader[ 9 on 4 ] = [ 0x71, 0x74, 0x20, 0x20 ]		// MOV
      		
      		bAudio					= False
      		
      	// Unable to determine the type of file using the header bytes: Use the extension
      	case Lower(fExtractPath( sFile, fExtension )[[ 2 to ]]) in ( "avi", "mkv", "mpg", "mpeg", "vob", "m2p", "mp4", "webm", "mov", "ts" )
      		
      		bAudio = False
      		
      	other case
      		ToastDisplay("File format not recognized", toastShort, vaMiddle, haCenter)  			
      		return
      		
      end
      
      // Stops any playback in progress
      PlaybackStop()
      
      // Loads the file
      MultimediaOpen(MM_PLAYER, sFile)
      if ErrorOccurred then
      	Error("Unable to open the file:", sFile, "", ErrorInfo)
      	return
      end
      
      // Starts playback
      PlaybackInitialize( sFile, bAudio ) 			
     type : 458752

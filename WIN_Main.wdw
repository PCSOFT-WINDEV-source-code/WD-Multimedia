     procedures :
      -
        name : MouseValue
        procedure_id : 1315892282028767535
        type_code : 14
        code : |1-
         // Summary: Returns the value of Slider control according to the position of the mouse cursor
         // Syntax:
         // MouseValue ()
         //
         // Parameters:
         //	None
         // Return value:
         // 	None
         //
         procedure mousevalue( X is int ) : int
         
         // Calculates the position based on the mouse coordinates
         // Offset the Slider button by half its width. Its center determines the position
         // The width used is the entire width of the slider minus the width of the slider cursor.
         return ( X - CoordinateEditorToScreen(12) ) * SLD_DURATION.MaxValue / ( SLD_DURATION.Width - CoordinateEditorToScreen(24) )
        type : 458752
      -
        name : ChangeValue
        procedure_id : 1315892282028833071
        type_code : 14
        code : |1+
         // Summary: Changes the value of the slider
         // Syntax:
         //ChangeValue (<POS>)
         //
         // Parameters:
         // 	POS: New position
         // Return value:
         // 	None
         //
         procedure ChangeValue( Pos )
         
         // If the value is different from the current position
         if SLD_DURATION <> Pos then
         	
         	// Changes the current position
         	SLD_DURATION = Pos
         	
         	// Executes the modification process
         	SLD_DURATION.ExecuteProcess(trtChange)	
         end
        type : 458752
